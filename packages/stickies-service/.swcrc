{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": ".",
    "paths": {
      "api/*": ["./src/api/*"],
      "lib/*": ["./src/lib/*"],
      "middleware/*": ["./src/middleware/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}